{
  "name": "mummy-money",
  "version": "1.0.0",
  "description": "Django Angular Admin",
  "scripts": {
    "install": "cd ./tasks/ && ./install.sh",
    "python-virtualenv": "cd ./backend/ && virtualenv --python=python3 venv",
    "createsuperuser": "cd ./tasks/ && ./createsuperuser.sh",
    "migrations": "cd ./tasks/ && ./migrations.sh",
    "load-data": "cd ./tasks/ && ./loaddata.sh",
    "start": "npm run start-backend & npm run start-frontend",
    "start-backend":  "cd ./backend/ && source venv/bin/activate && python3 manage.py runserver",
    "start-frontend": "cd ./mummy-money/ && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RodrigoRiquelme/mummy-money.git"
